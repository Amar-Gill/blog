---
interface Props {
  showStaticMarker?: boolean;
  showBasicControl?: boolean;
}

const { showStaticMarker = false, showBasicControl = false } = Astro.props;
import LeafletMap from "./LeafletMap";
---

<LeafletMap
  showStaticMarker={showStaticMarker}
  showBasicControl={showBasicControl}
  client:only="react"
/>
